{
    "name": "네이버 웹툰 북마크",
    "description": "네이버 웹툰 북마크",
    "version": "1.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icons/logo128.png",
        "32": "icons/logo128.png",
        "48": "icons/logo128.png",
        "128": "icons/logo128.png"
    },
    "homepage_url": "https://github.com/B-HS/Naver_Webtoon_Bookmarker",
    "action": {
        "default_title": "네이버 웹툰 북마크 Action Title",
        "default_popup": "popup/index.html"
    },
    "options_page": "options/index.html",
    "host_permissions": ["https://comic.naver.com/*"],
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["https://comic.naver.com/*"],
            "js": ["content-script.js"]
        }
    ]
}
